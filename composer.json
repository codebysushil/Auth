{
    "name": "codebysushil/auth",
    "description": "session based login system",
    "type": "project",
    "require": {
        "php": "^8.3",
        "phpmailer/phpmailer": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "symfony/var-dumper": "^7.3",
        "phpstan/phpstan": "^2.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload": {
        "psr-4": {
            "Tests": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "scripts": {
        "test:unit": "phpunit",
        "test:type": "phpstan analyse src",
        "test": ["@test:unit", "@test:type"]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
